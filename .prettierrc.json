	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"parser": "typescript",
	"printWidth": 120,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": true,
	"endOfLine": "auto",
	"overrides": [
		{
			"files": "*.css",
			"options": {
				"parser": "css"
			}
		}
	]
}
